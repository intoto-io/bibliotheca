{
  "name": "@intoto-dev/bibliotheca",
  "version": "0.23.4",
  "description": "Component library for water data visualisation. By Intoto",
  "engines": {
    "node": "^16.14.2",
    "npm": "^8.5.0"
  },
  "private": true,
  "workspaces": [
    "./packages/location-tooltip",
    "./packages/info-tooltip",
    "./packages/profile",
    "./packages/utils-is-unique-date",
    "./packages/graph",
    "./packages/utils-observation-helpers",
    "./packages/data-player",
    "./packages/eslint-config"
  ],
  "scripts": {
    "clean": "rimraf packages/**/lib",
    "test": "CI=true jest",
    "tsc": "npm exec --workspaces -- tsc",
    "prepare": "husky install",
    "start": "start-storybook -p 6006",
    "lint": "eslint packages --max-warnings 0",
    "build-storybook": "build-storybook",
    "prebuild-packages": "npm run clean",
    "build-packages": "npm run build --workspaces --if-present",
    "prepublish-all": "npm run build-packages",
    "publish-all": "node ./scripts/version_check.js && sh ./scripts/publish.sh"
  },
  "keywords": [],
  "author": "Intoto AS",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.5",
    "@mui/system": "^5.6.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "ncp": "^2.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@emotion/styled": "^11.6.0",
    "@intoto-dev/eslint-config-intoto": "file:packages/eslint-config",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "babel-jest": "^28.0.3",
    "commitlint": "^16.2.4",
    "emotion-theming": "^11.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.1",
    "tslib": "^2.3.1"
  }
}
